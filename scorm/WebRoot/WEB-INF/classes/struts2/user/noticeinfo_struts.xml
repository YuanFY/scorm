<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="noticeinfo" namespace="/" extends="json-default">
		<action name="getNoticeList" class="com.scorm.action.user.NoticeAction">
			<result name="success">/user/sysNotice.jsp</result>
		</action>
		
		<action name="noticeSearch" class="com.scorm.action.user.NoticeAction" method="noticeSearchAction">
			<result name="success">/user/sysNotice.jsp</result>
		</action>
		
		<action name="getNoticeInfo" class="com.scorm.action.user.NoticeAction" method="getNoticeInfoAction">
			<result name="success">/user/noticeInfo.jsp</result>
		</action>
	</package>
</struts>    
